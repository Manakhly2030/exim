 "html": "<style type=\"text/css\">\n    .tbspace > tbody > tr > td{ \n        padding: 0 2px 0 2px!important; \n        margin:0!important; \n        border-spacing: 0!important;\n    }\n</style>\n\n<div id=\"header-html\" class=\"hidden-pdf\">\n    {% if letter_head and not no_letterhead %}\n        <div class=\"letter-head\">{{ letter_head }}</div>\n    {% else %}\n        <div> </br></br></br> </div>\n    {% endif %}\n</div>\n<h4 align=\"center\"><strong>INFORMATION ABOUT VERIFIED GROSS MASS OF CONTAINER</strong></h4>\n<table border=\"1\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" class=\"tbspace\">\n    <tbody>\n        <tr>\n            <td width=\"5%\" nowrap=\"\" valign=\"bottom\">\n                <p>\n                   <b> Sr No.</b>\n                </p>\n            </td>\n            <td width=\"54%\" nowrap=\"\" valign=\"bottom\">\n                <p>\n                    <b>Details of information</b>\n                </p>\n            </td>\n            <td  width=\"40%\" nowrap=\"\" valign=\"bottom\">\n                <p>\n                  <b>  Particulars </b>\n                </p>\n            </td>\n        </tr>\n        <tr>\n            <td valign=\"bottom\">\n                <p>\n                    1*\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    BOOKING NO.\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                {{ doc.name }}\n            </td>\n        </tr>\n        <tr>\n            <td valign=\"bottom\">\n                <p>\n                    2*\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    Name of the shipper\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                   {{ doc.company|upper }}\n                </p>\n                <p>\n                    {{ doc.company_address_display|upper }}\n                </p>\n            </td>\n        </tr>\n        <tr>\n            <td valign=\"bottom\">\n                <p>\n                    3*\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    Shipper Registration /License no.( IEC No/CIN No)**\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    {{ frappe.db.get_value(\"Company\",doc.company,\"iec_code\") or ''}}\n                </p>\n            </td>\n        </tr>\n        <tr>\n            <td valign=\"bottom\">\n                <p>\n                    4*\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    Name and designation of official of the shipper authorized\n                </p>\n                <p>\n                    to sign document\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    Saji M. Divakaran\n                </p>\n            </td>\n        </tr>\n        <tr>\n            <td valign=\"bottom\">\n                <p>\n                    5*\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    24 x 7 contact details of authorised official of shipper\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    7227981477\n                </p>\n            </td>\n        </tr>\n        <tr>\n            <td valign=\"bottom\">\n                <p>\n                    6*\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    Container No.\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>{%- set container = [] -%}\n                    {% for row in doc.container_detail %}\n                    {%- if row.container_no not in container -%}\n                        {{ row.container_no }}\n                        {%- set _ = container.append(row.container_no) -%}\n                    {%- endif -%}\n                {%- endfor-%}\n                </p>\n            </td>\n        </tr>\n        <tr>\n            <td valign=\"bottom\">\n                <p>\n                    7*\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    Container Size ( TEU/FEU/other)\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    {%- set size = [] -%}\n                    {% for row in doc.container_detail %}\n                    {%- if row.size not in size -%}\n                        {{ row.size }}\n                        {%- set _ = size.append(row.size) -%}\n                    {%- endif -%}\n                    {% endfor %}\n                </p>\n            </td>\n        </tr>\n        <tr>\n            <td valign=\"bottom\">\n                <p>\n                    8*\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    Maximum permissible weight of container as per the CSC\n                    plate\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                {% if doc.container_detail %}\n                    {% if doc.container_detail[0].size == \"20'\" %}\n                        <p>\n                            MAX GR WT : 30480 KGS\n                        </p>\n                        <p>\n                            NET WT : 28230 KGS\n                        </p>\n                        <p>\n                            PAY LOAD : 28230 KGS\n                        </p>\n                    {% elif doc.container_detail[0].size == \"40'\" %}\n                        <p>\n                            MAX GR WT : 32500 KGS\n                        </p>\n                        <!--<p>-->\n                        <!--    NET WT : 28630 KGS-->\n                        <!--</p>-->\n                        <!--<p>-->\n                        <!--    PAY LOAD : 28630 KGS-->\n                        <!--</p>-->\n                    {% endif %}\n                {% endif %}\n            </td>\n        </tr>\n        <tr>\n            <td valign=\"bottom\">\n                <p>\n                    9*\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    Weighbridge registration no. &amp; Address of Weighbridge\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    M.J Weighbridge<br>Plot No. 4788, Near Water Tank, GIDC Main Road, Ankleshwar.\n                </p>\n            </td>\n        </tr>\n        <tr>\n            <td valign=\"bottom\">\n                <p>\n                    10*\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    Weighing Method (Method-1/Method-2)\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    METHOD-1\n                </p>\n            </td>\n        </tr>\n        <tr>\n            <td valign=\"bottom\">\n                <p>\n                    11*\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    Verified gross mass of container (method-1/method-2)\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    {{ doc.tare_weight_of_container or 0.0 }} {{ doc.items[0].uom|upper }} + {{ doc.gross_weight_with_pallet or 0.0 }} {{ doc.items[0].uom|upper }} = {{ doc.tare_weight_of_container|flt + doc.gross_weight_with_pallet|flt or 0.0 }} {{ doc.items[0].uom|upper }}\n                </p>\n            </td>\n        </tr>\n        <!--<tr>-->\n        <!--    <td valign=\"bottom\">-->\n        <!--        <p>-->\n        <!--            12*-->\n        <!--        </p>-->\n        <!--    </td>-->\n        <!--    <td  valign=\"bottom\">-->\n        <!--        <p>-->\n        <!--            Unit of Measurement (KGS/ MT / LBS )-->\n        <!--        </p>-->\n        <!--    </td>-->\n        <!--    <td  valign=\"bottom\">-->\n        <!--        <p>-->\n        <!--            {{ doc.items[0].uom|upper }}-->\n        <!--        </p>-->\n        <!--    </td>-->\n        <!--</tr>-->\n        <tr>\n            <td valign=\"bottom\">\n                <p>\n                    12*\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    Date and time of weighing\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    {{ doc.get_formatted(\"posting_date\") }}\n                </p>\n            </td>\n        </tr>\n        <tr>\n            <td valign=\"bottom\">\n                <p>\n                    13*\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    Weighing slip no.\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    {{ doc.weighing_slip_no or '' }}\n                </p>\n            </td>\n        </tr>\n        <tr>\n            <td valign=\"bottom\">\n                <p>\n                    14*\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    Type (Normal/Reefer/Hazardous/others)\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    Normal\n                </p>\n            </td>\n        </tr>\n        <tr>\n            <td valign=\"bottom\">\n                <p>\n                    15*\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    If Hazardous UN NO.IMDG class\n                </p>\n            </td>\n            <td  valign=\"bottom\">\n                <p>\n                    NIL\n                </p>\n            </td>\n        </tr>\n        <tr>\n            <td valign=\"bottom\">\n            </td>\n            <td  valign=\"bottom\">\n            </td>\n            <td  valign=\"bottom\">\n            </td>\n        </tr>\n    </tbody>\n</table>\n<table width=\"100%\">\n    <br>\n    <tr style=\"text-align: right;\">\n        <td>\n    \n    <p style=\"font-size:12px\">\n        Signature of authorised person of shipper\n    </p>\n    <br><br><br>\n    <p style=\"font-size:12px\">\n        Name : Narayan Organics Pvt. Ltd.(Unit-2)\n    </p>\n    <p style=\"font-size:12px\">\n        Date : {{ doc.get_formatted(\"posting_date\") }}\n    </p>\n    </td>\n    </tr>\n    <tr>\n        \n    <td width=\"60%\">\n        <p style=\"font-size:12px\">\n    <u>Remarks:</u>\n</p>\n<p style=\"font-size:12px\">\n    *Indicates mandatory field\n</p>\n<p style=\"font-size:12px\">\n    *Shippers not having IEC no. CIN No. may provide information as follows:\n</p>\n<p style=\"font-size:12px\">\n    Company - PAN NO.:\n</p>\n\n<p style=\"font-size:12px\">\n    Individuals: Indian National-\n</p>\n<p style=\"font-size:12px\">\n     Foreign National: - Passport No.& Country of isuue of passsport:India+ \n</p>\n    </td>\n    \n    </tr>\n</table>\n\n<div id=\"footer-html\" class=\"visible-pdf\">\n    {% if not no_letterhead and footer %}\n        <div class=\"letter-head-footer\">\n            {{ footer }}\n        </div>\n    {% endif %}\n</div>",
