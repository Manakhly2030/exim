 "doctype": "Print Format",
 "font_size": 14,
 "html": "<style>\r\n    .tbspace>tbody>tr>td {\r\n        padding: 0 2px 0 2px !important;\r\n        margin: 0 !important;\r\n        border-spacing: 0 !important;\r\n    }\r\n\r\n    .tbspace-sub>tbody>tr>td {\r\n        padding: 0 2px 0 2px !important;\r\n        margin: 0 !important;\r\n        border-spacing: 0 !important;\r\n    }\r\n    \r\n    .ontop {\r\n        border-top: 1px solid !important;\r\n    }\r\n\r\n    .onright {\r\n        border-right: 1px solid !important;\r\n    }\r\n\r\n    .onleft {\r\n        border-left: 1px solid !important;\r\n    }\r\n\r\n    .onbottom {\r\n        border-bottom: 1px solid !important;\r\n    }\r\n\r\n    .noright {\r\n        border-right: 0 !important;\r\n    }\r\n\r\n    .noleft {\r\n        border-left: 0 !important;\r\n    }\r\n\r\n    .noleftright {\r\n        border-right: 0 !important;\r\n        border-left: 0 !important;\r\n    }\r\n\r\n    .double {\r\n        border-bottom-style: double;\r\n        padding-bottom: 3px !important;\r\n    }\r\n\r\n    .notop {\r\n        border-top: 0 !important;\r\n    }\r\n\r\n    .nobottom {\r\n        border-bottom: 0 !important;\r\n    }\r\n\r\n    .notopbottom {\r\n        border-top: 0 !important;\r\n        border-bottom: 0 !important;\r\n        padding-top: 0 !important;\r\n        padding-bottom: 0 !important;\r\n        margin: 0 !important;\r\n        border-spacing: 0 !important;\r\n    }\r\n\r\n    td.v-align {\r\n        vertical-align: middle !important;\r\n    }\r\n\r\n    .bg {\r\n        background-color: #d2d2d2;\r\n    }\r\n\r\n    td.nopadding {\r\n        padding: 0 !important;\r\n    }\r\n\r\n    td.v-align-bt {\r\n        vertical-align: bottom !important;\r\n    }\r\n\r\n    .text-size {\r\n        font-size: 9px !important;\r\n    }\r\n\r\n    .bank {\r\n        line-height: 0.8 !important;\r\n    }\r\n</style>\r\n\r\n<div id=\"header-html\" class=\"hidden-pdf\">\r\n    {% if letter_head and not no_letterhead %}\r\n    <div style=\"margin-top:-1px;\" class=\"letter-head\">{{ letter_head }}</div>\r\n    {% else %}\r\n    <div style=\"margin-top:0px; height: 5px!important\"></div>\r\n    {% endif %}\r\n</div>\r\n<p class=\"text-size\" align=\"center\">\r\n    <strong>COMMERCIAL INVOICE</strong>\r\n</p>\r\n<table border=\"1\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" class=\"tbspace\">\r\n    <tbody>\r\n        <tr>\r\n            <td width=\"40%\" rowspan=\"4\" valign=\"top\">\r\n                <p class=\"text-size\"><strong>Exporter :</strong></p>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        {{ doc.company }}\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        {{ frappe.db.get_value(\"Company\",doc.company,'company_description') or '' }}\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'address_line1') or \"\" }}\r\n                </p>\r\n                {%- if frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'address_line2') -%}\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'address_line2') or \"\" }}\r\n                </p>\r\n                {%- endif -%}\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'city') or \"\" }}-\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'pincode') or \"\" }},\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'state') or \"\" }}\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'country') or \"\" }}\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    <strong>GST No. : {{ doc.company_gstin or '' }}</strong>\r\n                </p>\r\n                 <p class=\"text-size\">\r\n                    <strong>IEC Code: {{frappe.db.get_value(\"Company\",{'name':doc.company},'iec_code')  or '' }}</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    UDYAM REGISTRATION NUMBER:\r\n                    {{ frappe.db.get_value(\"Company\",doc.company,'msme_registration_number') or ''}}\r\n                </p>\r\n                  {% if doc.company_tax_id %}\r\n                                <p class=\"text-size\">\r\n                                    PAN NO : {{ doc.company_tax_id  }}\r\n                                </p>\r\n                                {% endif %}\r\n            </td>\r\n            <td width=\"30%\" valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Invoice No. &amp; Date :</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                   {{ doc.invoice_no or doc.name }}&nbsp;&nbsp;&nbsp; {{ doc.get_formatted(\"posting_date\") }}\r\n                </p>\r\n            </td>\r\n            <td width=\"30%\" valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Other Ref.</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.trade_agreement or '' }}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Buyer's Order No. & Date :</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.po_no|upper if doc.po_no else '' }}&nbsp;&nbsp;&nbsp;{{ doc.get_formatted(\"po_date\") or '' }}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\"><strong>Exporter Ref no and Date:</strong></p>\r\n               {% if doc.items[0].sales_order %}\r\n                <p class=\"text-size no-margin\">\r\n                    \r\n                    {{ frappe.db.get_value(\"Sales Order\",doc.items[0].sales_order,'order_no') or doc.items[0].sales_order or '' }}&nbsp;&nbsp;&nbsp;{{ frappe.format(frappe.db.get_value(\"Sales Order\",doc.items[0].sales_order,'transaction_date'), {'fieldtype': 'Date'}) }}\r\n                </p>\r\n                {% endif %}\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Country of origin:</strong>\r\n                </p>\r\n                <p class=\"text-size\">{{ doc.country_of_origin|upper if doc.country_of_origin else '' }}</p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\"><strong>Country of Destination :</strong></p>\r\n                <p class=\"text-size\">{{ doc.country_of_destination|upper if doc.country_of_destination else '' }}</p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Pre Carriage by\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.pre_carriage_by or '' }} <br>\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Place of Precarrier\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.place_of_precarrier or '' }}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        {% if doc.notify_address_display %}\r\n        <tr>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                     <strong>\r\n                                {{ frappe.db.get_value(\"Address\",doc.shipping_address_name,\"address_title\") or \"\" }}\r\n                            </strong>\r\n                    {{ doc.shipping_address|upper if doc.shipping_address else '' }}\r\n                </p>\r\n            </td>\r\n            <td colspan=\"2\">\r\n                <p class=\"text-size\">\r\n                    <strong>Notify</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.notify_address_display or ''}}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n               \r\n        <tr>\r\n            <td  rowspan=\"4\">\r\n                <p class=\"text-size\">\r\n                    <strong>Buyer</strong>(if other than Consignee)\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    <strong>{{ doc.customer_name }}</strong><br>\r\n                    {{ doc.address_display|upper if doc.address_display else '' }}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>Vessel/Flight No.</strong></p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.vessel_no|upper if doc.vessel_no else '' }}\r\n                    <br>\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\"><strong>Port of Loading</strong></p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.port_of_loading|upper if doc.port_of_loading else '' }}\r\n                    {% if frappe.db.get_value(\"Port Details\",doc.port_of_loading,\"country\") %}\r\n                    ,{{ frappe.db.get_value(\"Port Details\",doc.port_of_loading,\"country\")|upper if frappe.db.get_value(\"Port Details\",doc.port_of_loading,\"country\") else '' }}\r\n                    {% endif %}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n              <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Port of Discharge\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.port_of_discharge|upper if doc.port_of_discharge else '' }}\r\n                    {% if frappe.db.get_value(\"Port Details\",doc.port_of_discharge,\"country\") %}\r\n                    ,{{ frappe.db.get_value(\"Port Details\",doc.port_of_discharge,\"country\")|upper if frappe.db.get_value(\"Port Details\",doc.port_of_discharge,\"country\") else '' }}\r\n                    {% endif %}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Final Destination\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.final_destination|upper if doc.final_destination else '' }}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td>\r\n                \r\n                <p class=\"text-size\">\r\n                    <strong>Terms of Delivery and Payment</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    PAYMENT : {{ doc.payment_terms_template|upper if doc.payment_terms_template else '' }}\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    DELIVERY :  \r\n                     {% if doc.shipping_terms == \"FOB\" %}\r\n                    {{ doc.shipping_terms or ''}} {{ doc.port_of_loading or '' }}\r\n                    {% elif doc.shipping_terms == \"Ex-Works\"%} \r\n                    {{ doc.shipping_terms or ''}}\r\n                    {% else %}\r\n                    {{ doc.shipping_terms or ''}} {{ doc.port_of_discharge or '' }}\r\n                    {% endif %}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>LC Number & Date:</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                   {{ frappe.db.get_value(\"Contract Term\",doc.contract_and_lc,\"lc_no\") or ''}}&nbsp;&nbsp;&nbsp;{{ frappe.format(frappe.db.get_value(\"Contract Term\",doc.contract_and_lc,\"date_of_issue\"), {'fieldtype': 'Date'}) or '' }}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n         \r\n      \r\n        {% else %}\r\n        \r\n        <tr>\r\n            <td  rowspan=\"2\">\r\n                <p class=\"text-size\">\r\n                    <strong>Consignee :</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                     <strong>\r\n                                {{ frappe.db.get_value(\"Address\",doc.shipping_address_name,\"address_title\") or \"\" }}\r\n                            </strong> <br>\r\n                    {{ doc.shipping_address|upper if doc.shipping_address else '' }}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>Vessel/Flight No.</strong></p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.vessel_no|upper if doc.vessel_no else '' }}\r\n                    <br>\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\"><strong>Port of Loading</strong></p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.port_of_loading|upper if doc.port_of_loading else '' }}\r\n                    {% if frappe.db.get_value(\"Port Details\",doc.port_of_loading,\"country\") %}\r\n                    ,{{ frappe.db.get_value(\"Port Details\",doc.port_of_loading,\"country\")|upper if frappe.db.get_value(\"Port Details\",doc.port_of_loading,\"country\") else '' }}\r\n                    {% endif %}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Port of Discharge\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.port_of_discharge|upper if doc.port_of_discharge else '' }}\r\n                    {% if frappe.db.get_value(\"Port Details\",doc.port_of_discharge,\"country\") %}\r\n                    ,{{ frappe.db.get_value(\"Port Details\",doc.port_of_discharge,\"country\")|upper if frappe.db.get_value(\"Port Details\",doc.port_of_discharge,\"country\") else '' }}\r\n                    {% endif %}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Final Destination\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.final_destination|upper if doc.final_destination else '' }}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td rowspan=\"2\">\r\n                <p class=\"text-size\">\r\n                    <strong>Buyer</strong>(if other than Consignee)\r\n                </p>\r\n                 <p class=\"text-size\">\r\n                    <strong>{{ doc.customer_name }}</strong><br>\r\n                    {{ doc.address_display|upper if doc.address_display else '' }}\r\n                </p>\r\n            </td>\r\n             <td >\r\n                 \r\n                <p class=\"text-size\">\r\n                    <strong>Terms of Delivery and Payment</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    PAYMENT : {{ doc.payment_terms_template|upper if doc.payment_terms_template else '' }}\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    DELIVERY :  \r\n                    {% if doc.shipping_terms == \"FOB\" %}\r\n                    {{ doc.shipping_terms or ''}}-{{ doc.port_of_loading or '' }}\r\n                    {% elif doc.shipping_terms == \"Ex-Works\"%} \r\n                    {{ doc.shipping_terms or ''}}\r\n                    {% else %}\r\n                    {{ doc.shipping_terms or ''}}-{{ doc.port_of_discharge or '' }}\r\n                    {% endif %}\r\n                    \r\n                </p>\r\n            </td>\r\n        \r\n            <td >\r\n                <p class=\"text-size\">\r\n                    <strong>LC Number & Date:</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                   {{ frappe.db.get_value(\"Contract Term\",doc.contract_and_lc,\"lc_no\") or ''}}&nbsp;&nbsp;&nbsp;{{ frappe.format(frappe.db.get_value(\"Contract Term\",doc.contract_and_lc,\"date_of_issue\"), {'fieldtype': 'Date'}) or '' }}\r\n                </p>\r\n                \r\n            </td>\r\n           \r\n        </tr>\r\n        {% endif %}\r\n         <tr>\r\n             <td colspan=\"2\">\r\n                 \r\n              <p class=\"text-size\">\r\n                    <strong>LC Other Details :</strong>{{ doc.lc_other_details or '' }}\r\n                </p>\r\n             </td>\r\n        </tr>\r\n        <tr>\r\n            <td width=\"100%\" colspan=\"3\" class=\"nopadding\" style=\"padding: 0!important;\">\r\n            {% set length = doc.items|length %} {% set ln = 5 * doc.items|length %} {% set def = 150 %}\r\n                <table border=\"1\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" class=\"tbspace-sub\">\r\n                    <tbody>\r\n                        <tr>\r\n                            <td width=\"16%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>MARKS & No</strong>\r\n                                </p>\r\n                            </td>\r\n                            <td width=\"10%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>No. & Kind of<br>Packages</strong>\r\n                                </p>\r\n                            </td>\r\n                            <td width=\"22%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>Description of Goods</strong>\r\n                                </p>\r\n                               \r\n                            </td>\r\n                            <td width=\"8%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>Qty {{ doc.items[0].uom }}\r\n                                    </strong>\r\n                                </p>\r\n                            </td>\r\n                            <td width=\"7%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>Rate<br>{{ doc.currency }}/ {{ doc.items[0].uom }}</strong>\r\n                                </p>\r\n                            </td>\r\n                            <td width=\"9%\" class=\"v-align\" align=\"center\">\r\n                                <p class=\"text-size\">\r\n                                    <strong>Amount</strong>\r\n                                    <strong>{{ doc.currency }}</strong>\r\n                                </p>\r\n                            </td>\r\n                        </tr>\r\n\r\n                        {%- for row in doc.items -%}\r\n                        <tr style=\"height: {{ ([ln,def]|max) }}px!important;\">\r\n                            \r\n                            <td valign=\"top\" align=\"left\" class=\"notopbottom\">\r\n                                <p class=\"text-size\">\r\n                                   {% if row.marks_and_no %}\r\n                                    {% if loop.last %}{{ row.marks_and_no.replace(\"\\n\",\"<br>\") }} {% else %} {{ row.marks_and_no.replace(\"\\n\",\"<br>\") }} {% endif %}\r\n                                    {% endif %}\r\n                                </p>\r\n                            </td>\r\n                        \r\n                            <td valign=\"top\" align=\"center\" class=\"notopbottom\">\r\n                                <p class=\"text-size\">\r\n                                      \r\n                                       {{row.no_of_packages or \"\"}}  {{ row.packaging_material or ''}} {% if row.packing_size %} of {% endif %}\r\n                                            {{ row.packing_size or ''}} {{row.uom or \"\"}}\r\n                                           \r\n                                            {% if row.palleted %}\r\n                                               \r\n                                                each on {{row.total_pallets or \"\"}} pallets\r\n                                            {% endif %}\r\n                                        \r\n                                    <!-- {% if not row.dont_show_package_no %}\r\n                                    <br> {{ row.packaging_material or ''}} : {{ row.packages_from }} to\r\n                                    {{ row.packages_to }}\r\n                                    {% endif %} -->\r\n\r\n                                </p>\r\n                            </td>\r\n  <td  class=\"notopbottom onright text-size\">\r\n                                     <p class=\"text-size no-margin\">\r\n                                        {{row.description or row.item_code or \"\"}}\r\n                                    </p>\r\n                                \r\n                                <p class=\"text-size no-margin\">\r\n                                    (HS Code: {{ row.gst_hsn_code or '' }})\r\n                                </p>\r\n                               \r\n                            </td>\r\n\r\n                            <td valign=\"top\" align=\"center\" class=\"notopbottom\">\r\n                                <p class=\"text-size\">\r\n                                    {{ row.qty or '' }}\r\n                                </p>\r\n                            </td>\r\n                            <td valign=\"top\" align=\"right\" class=\"notopbottom\">\r\n                                <p class=\"text-size\">\r\n                                    {{ row.get_formatted(\"rate\") or '' }}\r\n                                </p>\r\n                            </td>\r\n                            <td class=\"notopbottom\" align=\"right\">\r\n                                <p class=\"text-size\">\r\n                                    {{ row.get_formatted(\"amount\") or '' }}\r\n                                </p>\r\n                            </td>\r\n                            <!--<td class=\"notopbottom\" align=\"right\">-->\r\n                            <!--    <p class=\"text-size\">-->\r\n                            <!--        {{ row.get_formatted(\"base_amount\") or '' }}-->\r\n                            <!--    </p>-->\r\n                            <!--</td>-->\r\n                            <!--<td class=\"notopbottom\" align=\"right\">-->\r\n                            <!--    <p class=\"text-size\">-->\r\n                            <!--        {{ row.get_formatted(\"igst_amount\") or '' }}-->\r\n                            <!--    </p>-->\r\n                            <!--</td>-->\r\n                        </tr>\r\n                        {% endfor %}\r\n                        <tr>\r\n                            <td valign=\"top\" align=\"left\" class=\"notopbottom\">\r\n                          \r\n                            </td>\r\n                            <td valign=\"top\" align=\"center\" class=\"notopbottom\">\r\n                            </td>\r\n                            <td valign=\"top\" class=\"notopbottom\">\r\n                               <!--<p class=\"text-size\">-->\r\n                               <!--    {% if doc.items[0].palleted %}-->\r\n                               <!--         (ALL {{ doc.total_packages|upper if doc.total_packages else '' }} PACKAGES ARE-->\r\n                               <!--         ON {{ doc.total_pallets|int or '' }} PALLETS)-->\r\n                               <!--         {% endif %}-->\r\n                               <!--     </p>-->\r\n                               <br>\r\n                                <p style=\"font-size: 10px!important\">\r\n                                    <b>Rex Number : \r\n                                    {{frappe.db.get_value(\"Company\", doc.company,\"rex_number\") or \"\"}}</b>\r\n                                </p>\r\n                               <p style=\"font-size: 10px!important\">\r\n                                    CONTAINER NO:{%- set container = [] -%}\r\n                                     {% for row in doc.container_detail %}\r\n                                     {%- if row.container_no not in container -%}\r\n                                        {{ row.container_no }}\r\n                                    {%- set _ = container.append(row.container_no) -%}\r\n                                    {%- endif -%}\r\n                                    {%- endfor-%}\r\n                                </p>\r\n                               \r\n                                 <p style=\"font-size: 10px!important\">\r\n                                    B/L NO: {{ doc.bl_no or \"\"}}\r\n                                </p>\r\n             