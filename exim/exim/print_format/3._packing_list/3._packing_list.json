 "doctype": "Print Format",
 "font_size": 14,
 "html": "<style>\r\n    .tbspace>tbody>tr>td {\r\n        padding: 0 2px 0 2px !important;\r\n        margin: 0 !important;\r\n        border-spacing: 0 !important;\r\n    }\r\n\r\n    .tbspace-sub>tbody>tr>td {\r\n        padding: 0 4px 0 4px !important;\r\n        margin: 0 !important;\r\n        border-spacing: 0 !important;\r\n    }\r\n\r\n   \r\n\r\n    table {\r\n        border-collapse: collapse;\r\n        border-spacing: 00 !important;\r\n    }\r\n\r\n    .ontop {\r\n        border-top: 1px solid !important;\r\n    }\r\n\r\n    .onright {\r\n        border-right: 1px solid !important;\r\n    }\r\n\r\n    .onleft {\r\n        border-left: 1px solid !important;\r\n    }\r\n\r\n    .onbottom {\r\n        border-bottom: 1px solid !important;\r\n    }\r\n\r\n    .noright {\r\n        border-right: 0 !important;\r\n    }\r\n\r\n    .noleft {\r\n        border-left: 0 !important;\r\n    }\r\n\r\n    .noleftright {\r\n        border-right: 0 !important;\r\n        border-left: 0 !important;\r\n    }\r\n\r\n    .double {\r\n        border-bottom-style: double;\r\n        padding-bottom: 3px !important;\r\n    }\r\n\r\n    .notop {\r\n        border-top: 0 !important;\r\n    }\r\n\r\n    .nobottom {\r\n        border-bottom: 0 !important;\r\n    }\r\n\r\n    .notopbottom {\r\n        border-top: 0 !important;\r\n        border-bottom: 0 !important;\r\n        padding-top: 0 !important;\r\n        padding-bottom: 0 !important;\r\n        margin: 0 !important;\r\n        border-spacing: 0 !important;\r\n    }\r\n\r\n    td.v-align {\r\n        vertical-align: middle !important;\r\n    }\r\n\r\n    .bg {\r\n        background-color: #d2d2d2;\r\n    }\r\n\r\n    td.nopadding {\r\n        padding: 0 !important;\r\n    }\r\n\r\n    td.v-align-bt {\r\n        vertical-align: bottom !important;\r\n    }\r\n\r\n    .text-size {\r\n        font-size: 9px !important\r\n    }\r\n</style>\r\n<div id=\"header-html\" class=\"hidden-pdf\">\r\n    {% if letter_head and not no_letterhead %}\r\n    <div class=\"letter-head\">{{ letter_head }}</div>\r\n    {% endif %}\r\n</div>\r\n<div>\r\n    <p class=\"text-size\" align=\"center\">\r\n        <strong>CUSTOM PACKING LIST</strong>\r\n    </p>\r\n</div>\r\n<div>\r\n    <table border=\"1\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" class=\"tbspace\">\r\n        <tbody>\r\n        <tr>\r\n            <td width=\"40%\" rowspan=\"3\" valign=\"top\">\r\n                <p class=\"text-size\"><strong>Exporter :</strong></p>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        {{ doc.company }}\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'address_line1') or \"\" }}\r\n                </p>\r\n                {%- if frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'address_line2') -%}\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'address_line2') or \"\" }}\r\n                </p>\r\n                {%- endif -%}\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'city') or \"\" }}-\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'pincode') or \"\" }},\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'state') or \"\" }}\r\n                </p>\r\n\r\n                <p class=\"text-size\">\r\n                    <strong>GST No. : {{ doc.company_gstin or '' }}</strong>\r\n                </p>\r\n                 <p class=\"text-size\">\r\n                    UDYAM REGISTRATION NUMBER:\r\n                    {{ frappe.db.get_value(\"Company\",doc.company,'msme_registration_number') or ''}}\r\n                </p>\r\n                \r\n                    {% if doc.company_tax_id %}\r\n                                <p class=\"text-size\">\r\n                                    PAN NO : {{ doc.company_tax_id  }}\r\n                                </p>\r\n                                {% endif %}\r\n        \r\n            </td>\r\n            <td width=\"30%\" valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Invoice No. &amp; Date :</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.name }}&nbsp;&nbsp;&nbsp; {{ doc.get_formatted(\"posting_date\") }}\r\n                </p>\r\n            </td>\r\n            <td width=\"30%\" valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Trade Agreement</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.trade_agreement or '' }}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Buyer's Order No. & Date :</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.po_no|upper if doc.po_no else '' }}&nbsp;&nbsp;&nbsp;{{ doc.get_formatted(\"po_date\") or '' }}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\"><strong>Exporter Ref no and Date:</strong></p>\r\n                 {% if doc.items[0].sales_order %}\r\n                <p class=\"text-size no-margin\">\r\n                    \r\n                    {{ frappe.db.get_value(\"Sales Order\",doc.items[0].sales_order,'order_no') or doc.items[0].sales_order or '' }}&nbsp;&nbsp;&nbsp;{{ frappe.format(frappe.db.get_value(\"Sales Order\",doc.items[0].sales_order,'transaction_date'), {'fieldtype': 'Date'}) }}\r\n                </p>\r\n                {% endif %}\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Country of origin:</strong>\r\n                </p>\r\n                <p class=\"text-size\">{{ doc.country_of_origin|upper if doc.country_of_origin else '' }}</p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\"><strong>Country of Final Destination :</strong></p>\r\n                <p class=\"text-size\">{{ doc.country_of_destination|upper if doc.country_of_destination else '' }}</p>\r\n            </td>\r\n        </tr>\r\n        \r\n        <tr>\r\n            <td rowspan=\"2\">\r\n                <p class=\"text-size\">\r\n                    <strong>Consignee :</strong>\r\n                </p>\r\n               <p class=\"text-size\"><strong>TO THE ORDER</strong></p>\r\n                <p class=\"text-size\"><strong>{{ doc.country_of_destination }}</strong></p>\r\n\r\n                <!--<p class=\"text-size no-margin\">-->\r\n                <!--    {{ doc.port_of_discharge|upper if doc.port_of_discharge else '' }}-->\r\n                <!--</p>-->\r\n                 \r\n            </td>\r\n              <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Pre Carriage by\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.pre_carriage_by or '' }}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Place of Precarrier\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.place_of_precarrier or '' }}\r\n                </p>\r\n            </td>\r\n     \r\n            \r\n        \r\n        </tr>\r\n        <tr>\r\n          <td>\r\n                <p class=\"text-size\">\r\n                    <strong>Vessel/Flight No.</strong></p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.vessel_no|upper if doc.vessel_no else '' }}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\"><strong>Port of Loading</strong></p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.port_of_loading|upper if doc.port_of_loading else '' }}\r\n                    {% if frappe.db.get_value(\"Port Details\",doc.port_of_loading,\"country\") %}\r\n                    ,{{ frappe.db.get_value(\"Port Details\",doc.port_of_loading,\"country\")|upper if frappe.db.get_value(\"Port Details\",doc.port_of_loading,\"country\") else '' }}\r\n                    {% endif %}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td width=\"40%\" rowspan=\"3\">\r\n                <p class=\"text-size\">\r\n                    <strong>Buyer</strong>(if other than Consignee)\r\n                </p>\r\n                 <p class=\"text-size\"><strong>TO THE ORDER</strong></p>\r\n                <p class=\"text-size no-margin\">\r\n                {{ doc.final_destination|upper if doc.final_destination else '' }}\r\n                                                <p class=\"text-size\"><strong>{{ doc.country_of_destination }}</strong></p>\r\n\r\n                    <!--{{ doc.shipping_address|upper if doc.shipping_address else ''}}-->\r\n                </p>\r\n            </td>\r\n              <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Port of Discharge\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.port_of_discharge|upper if doc.port_of_discharge else '' }}\r\n                    {% if frappe.db.get_value(\"Port Details\",doc.port_of_discharge,\"country\") %}\r\n                    ,{{ frappe.db.get_value(\"Port Details\",doc.port_of_discharge,\"country\")|upper if frappe.db.get_value(\"Port Details\",doc.port_of_discharge,\"country\") else '' }}\r\n                    {% endif %}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Final Destination\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.final_destination|upper if doc.final_destination else '' }}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        \r\n        <tr>\r\n            <td colspan=\"2\">\r\n                <p class=\"text-size\">\r\n                    <strong>LC Number & Date:</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Contract Term\",doc.contract_and_lc,\"lc_no\") or ''}}&nbsp;&nbsp;&nbsp;{{ frappe.format(frappe.db.get_value(\"Contract Term\",doc.contract_and_lc,\"date_of_issue\"), {'fieldtype': 'Date'}) or '' }}\r\n                </p>\r\n            </td>\r\n            \r\n        </tr>\r\n        <tr>\r\n            <td colspan=\"2\">\r\n                <p class=\"text-size\">\r\n                    <strong>LC Details :</strong> {{ doc.lc_other_details or '' }}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n            <tr>\r\n                <td width=\"100%\" colspan=\"3\" class=\"nopadding\" style=\"padding: 0!important;\">   \r\n                {% set length = doc.items|length %} {% set ln = 5 * doc.items|length %} {% set def = 200 %}\r\n                    <table border=\"1\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" class=\"tbspace-sub\">\r\n                        <tbody>\r\n                            <tr>\r\n                                <td width=\"19%\" class=\"v-align\">\r\n                                    <p class=\"text-size\" align=\"center\">\r\n                                        <strong>MARKS & No/<br>Container No.</strong>\r\n                                    </p>\r\n                                </td>\r\n                                <td width=\"13%\" class=\"v-align\">\r\n                                    <p class=\"text-size\" align=\"center\">\r\n                                        <strong>No. & Kind of<br>Packages</strong>\r\n                                    </p>\r\n                                </td>\r\n                                <td width=\"30%\" class=\"v-align\">\r\n                                    <p class=\"text-size\" align=\"center\">\r\n                                        <strong>Description of Goods</strong>\r\n                                    </p>\r\n                                    \r\n                                </td>\r\n\r\n                                <td width=\"10%\" class=\"v-align\">\r\n                                    <p class=\"text-size\" align=\"center\">\r\n                                        <strong>Net Weight <br>TOTAL {{ doc.items[0].uom }}\r\n                                        </strong>\r\n                                    </p>\r\n                                </td>\r\n                                <td width=\"10%\" class=\"v-align\" align=\"center\">\r\n                                    <p class=\"text-size\">\r\n                                        <strong>TARE WT<br> {{ doc.items[0].uom }}</strong>\r\n                                    </p>\r\n                                </td>\r\n                                <td width=\"10%\" class=\"v-align\" align=\"center\">\r\n                                    <p class=\"text-size\">\r\n                                        <strong>GROSS WT<br>{{ doc.items[0].uom }}</strong>\r\n                                    </p>\r\n                                </td>\r\n                            </tr>\r\n\r\n                            {%- for row in doc.items -%}\r\n                            {%- if row.page_break -%}\r\n                        </tbody>\r\n                    </table>\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td width=\"100%\" colspan=\"3\" class=\"nopadding\" style=\"padding: 0!important;\">\r\n                    <table border=\"1\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" class=\"tbspace-sub\">\r\n                        <tbody>\r\n                            <tr>\r\n                                <td width=\"19%\" class=\"v-align\">\r\n                                    <p class=\"text-size\" align=\"center\">\r\n                                        <strong>MARKS & No/<br>Container No.</strong>\r\n                                    </p>\r\n                                </td>\r\n                                <td width=\"13%\" class=\"v-align\">\r\n                                    <p class=\"text-size\" align=\"center\">\r\n                                        <strong>No. & Kind of<br>Packages</strong>\r\n                                    </p>\r\n                                </td>\r\n                                <td width=\"30%\" class=\"v-align\">\r\n                                    <p class=\"text-size\" align=\"center\">\r\n                                        <strong>Description of Goods</strong>\r\n                                    </p>\r\n                                    <p class=\"text-size\" align=\"center\">\r\n                                        <strong>{% if doc.description_of_goods %}{{ doc.description_of_goods.replace(', ','<br>')}}{% else %}SYNTHETIC\r\n                                            ORGANIC DYES<br>(REACTIVE DYES){% endif %}</strong>\r\n                                    </p>\r\n                                </td>\r\n\r\n                                <td width=\"10%\" class=\"v-align\">\r\n                                    <p class=\"text-size\" align=\"center\">\r\n                                        <strong>Net Weight <br>TOTAL {{ doc.items[0].uom }}\r\n                                        </strong>\r\n                                    </p>\r\n                                </td>\r\n                                <td width=\"10%\" class=\"v-align\" align=\"center\">\r\n                                    <p class=\"text-size\">\r\n                                        <strong>TARE WT<br> {{ doc.items[0].uom }}</strong>\r\n                                    </p>\r\n                                </td>\r\n                                <td width=\"10%\" class=\"v-align\" align=\"center\">\r\n                                    <p class=\"text-size\">\r\n                                        <strong>GROSS WT<br>{{ doc.items[0].uom }}</strong>\r\n                                    </p>\r\n                                </td>\r\n                            </tr>\r\n                            {% endif %}\r\n                            <tr style=\"height: {{ ([ln,def]|max) }}px!important;\">\r\n                                \r\n                                <td valign=\"top\"  class=\"notopbottom\" >\r\n                                    <p class=\"text-size\">\r\n                                         {% if row.marks_and_no %}\r\n                                            {{ row.marks_and_no.replace(\"\\n\",\"<br>\") }} \r\n                                    {% endif %}\r\n                                    \r\n                                    </p>\r\n                                </td>\r\n                                <td valign=\"top\" align=\"center\" class=\"notopbottom\">\r\n                                    <p class=\"text-size\">\r\n                                        \r\n                                           {{row.no_of_packages or \"\"}}  {{ row.packaging_material or ''}} {% if row.packing_size %} of {% endif %}\r\n                                            {{ row.packing_size or ''}} {{row.uom or \"\"}}\r\n                                           \r\n                                            {% if row.palleted %}\r\n                                               \r\n                                                each on {{row.total_pallets or \"\"}} pallets\r\n                                            {% endif %}\r\n\r\n                                    </p>\r\n                                </td>\r\n                                <td valign=\"top\" class=\"notopbottom text-size\">\r\n                                    {% if row.item_custom_name %}\r\n                                    <p class=\"text-size no-margin\">\r\n                                        {{row.item_custom_name or \"\"}}\r\n                                    </p>\r\n                                {% else %}\r\n                                    <p  class=\"text-size\">\r\n                                         {{ row.description or \"\"}}\r\n                                    </p>\r\n                                {% endif %}\r\n                                    <p class=\"text-size\">\r\n                                       (HS Code: {{ row.gst_hsn_code or '' }})\r\n                                    </p>\r\n                                    {% if row.advance_authorisation_license %}\r\n                                        <p class=\"text-size\"> Advance Authorisation Number- {{ row.advance_authorisation_license }}</p>\r\n                                    {% endif %}\r\n                                </td>\r\n                                \r\n\r\n                                <td valign=\"top\" align=\"center\" class=\"notopbottom\">\r\n                                    <p class=\"text-size\">\r\n                                        {{ row.qty or '' }} {{ row.uom or \"Uom\"}}\r\n                                    </p>\r\n                                </td>\r\n                                <td class=\"notopbottom\" align=\"center\">\r\n                                    <p class=\"text-size\">\r\n                                        {{ row.total_tare_weight or '' }} {{ row.uom or \"Uom\"}}\r\n                                    </p>\r\n                                </td>\r\n                                <td class=\"notopbottom\" align=\"center\">\r\n                                    <p class=\"text-size\">\r\n                                        {{ row.gross_wt or '' }} {{ row.uom or \"Uom\"}}\r\n                                    </p>\r\n                                </td>\r\n                            </tr>\r\n                            {% endfor %}\r\n                            <tr>\r\n                                <td valign=\"top\" align=\"center\" class=\"notopbottom\">\r\n                                </td>\r\n                                <td valign=\"top\" align=\"center\" class=\"notopbottom\">\r\n                                </td>\r\n                                <td valign=\"top\" class=\"notopbottom\">\r\n                                 \r\n                                    <p class=\"text-size\">\r\n                                        <strong>{% if doc.dbk_sr_no %} Export Under Duty DBK Scheme {% endif %}</strong>\r\n                                    </p>\r\n                                </td>\r\n\r\n                                <td valign=\"top\" align=\"right\" class=\"notopbottom\">\r\n\r\n                                </td>\r\n                                <td class=\"notopbottom\" align=\"right\">\r\n\r\n                                </td>\r\n                                <td class=\"notopbottom\" align=\"right\">\r\n\r\n                                </td>\r\n                            </tr>\r\n                            <tr>\r\n                                <td valign=\"top\" align=\"center\">\r\n                                </td>\r\n                                <td valign=\"top\" align=\"center\">\r\n                                    <p class=\"text-size\"><strong>TOTAl {{ doc.total_pallets|upper if doc.total_pallets else '' }} Pallets</strong></p>\r\n                                </td>\r\n                                <td valign=\"top\" align=\"center\">\r\n                                    <!--<p class=\"text-size\" align=\"center\"> -->\r\n                                    <!--    <strong>D.B.K SR NO.{{ doc.dbk_sr_no or ''}}</strong>-->\r\n                                    <!--</p>-->\r\n                                </td>\r\n\r\n                                <td>\r\n                                    <p class=\"text-size\" align=\"center\">\r\n                                        {{ doc.total_qty or '' }}&nbsp;{{ doc.items[0].uom }}\r\n                                    </p>\r\n                                </td>\r\n                                <td>\r\n                                    <p class=\"text-size\" align=\"center\">\r\n                                        {{ doc.total_tare_wt or '' }}&nbsp;{{ doc.items[0].uom }}\r\n                                    </p>\r\n                                </td>\r\n                                <td>\r\n                                    <p class=\"text-size\" align=\"center\">\r\n                                        {{ doc.total_gr_wt|float(2) or '' }}&nbsp;{{ doc.items[0].uom }}\r\n                                    </p>\r\n                                </td>\r\n                            </tr>\r\n                        </tbody>\r\n                    </table>\r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td width=\"100%\" colspan=\"2\" class=\"notop noright nobottom\" align=\"left\">\r\n                    <!--{%- if doc.trade_agreement -%}-->\r\n                    <!--<p class=\"text-size\">-->\r\n                    <!--    <strong> Agreement Code: {{ doc.trade_agreement or \"\" }}</strong>-->\r\n\r\n                    <!--</p>-->\r\n                    <!--{% endif %}-->\r\n                    <!--{%- if doc.export_type == 'With Payment of Tax'  -%}-->\r\n                    <!--<p class=\"text-size\">GST TYPE: GSN</p>-->\r\n                    <!--<p class=\"text-size\">IGST PAYMENT : {{doc.igst_payment_status or ''}}</p>-->\r\n\r\n                    <!--<p class=\"text-size\">END USE: GNX-200</p>-->\r\n                    <!--<p class=\"text-size\">-->\r\n                    <!--    SUPPLY MEANT FOR EXPORT ON PAYMENT OF INTEGRATED WITH TAX-->\r\n                    <!--</p>-->\r\n                    <!--{% endif %}-->\r\n                    <p class=\"text-size\">WE INTEND TO CLIAM UNDER RODTEPY SCHEME</p>\r\n                    <p class=\"text-size\">{{doc.remarks or ''}}</p>\r\n                </td>\r\n                <td>\r\n               \r\n                    <!--<p class=\"text-size\" align=\"right\"><b>(ALL {{ doc.total_packages|upper if doc.total_packages else '' }} PACKAGES ARE ON-->\r\n                    <!--        {{ doc.total_pallets|int or '' }} PALLETS)</b></p>-->\r\n                    \r\n                    <p class=\"text-size\" align=\"right\"> Gross WT : {{ doc.total_gr_wt|float(2) or '' }}&nbsp;{{ doc.items[0].uom }}</p>\r\n                    <p class=\"text-size\" align=\"right\"> Net WT : {{ doc.total_qty|float(2) or '' }}&nbsp;{{ doc.items[0].uom }}\r\n                </p>\r\n                    <!--<p class=\"text-size\" align=\"right\"> Pallet WT :-->\r\n                    <!--    {% if doc.total_pallets %}{{ doc.pallet_weight }}{% endif %}</p>-->\r\n                    <!--<p class=\"text-size\" align=\"right\"> Weight after Palletization :-->\r\n                    <!--    {% if doc.total_pallets %}{{ doc.weight_after_palletization }}{% endif %}</p>-->\r\n                  \r\n                </td>\r\n            </tr>\r\n            <tr>\r\n                <td  class=\"notop v-align-bt\" align=\"left\">\r\n                    {% if doc.print_rex_declaration%}\r\n                        <p class=\"text-size\"><strong>Rex Declaration- </strong>\r\n                            {{ doc.rex_details }}\r\n                        </p>\r\n                    {% else %}\r\n                         <p class=\"text-size\" style=\"font-size:8px !important;\">\r\n                                \r\n                                <!--This Invoice shows the actual price of the goods described and that all the particulars are-->\r\n                                <!--true and correct.<br>-->\r\n                                <!--E. & O.E.<br>-->\r\n                                <!--1. SUBJECT TO AHMEDABAD JURISDICTION<br>-->\r\n                                <!--2. INTEREST @18% WILL CHARGED IF BILLS NOT PAID WITHIN DUE PERIOD.<br>-->\r\n                                <!--3. OUR RISK & RESPONSIBILITY CEASES AS SOON AS THE GOODS LEAVE OUR PREMISES.<br>-->\r\n                                <!--4. ANY COMPLAINT REGARDING THIS INVOICE SHOULD BE DRAW ATTENTION WITHIN 3 DAYS OF RECEIPT-->\r\n                                <!--THIS INVOICE-->\r\n                            </p>\r\n                    {% endif %}\r\n                </td>\r\n                <td colspan=\"2\" align=\"right\">\r\n                    <p style=\"font-size: 15px!important\"><strong>For {{ doc.company }}</strong>\r\n                    </p><br><br><br>\r\n                    <p class=\"text-size\">\r\n                        <strong>PARTNER / AUTHORISED SIGNATORY</strong>\r\n                    </p>\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n</div>\r\n<div id=\"footer-html\" class=\"visible-pdf\">\r\n    {% if not no_letterhead and footer %}\r\n    <div class=\"letter-head-footer\">\r\n        {{ footer }}\r\n    </div>\r\n    {% endif %}\r\n</div>",
