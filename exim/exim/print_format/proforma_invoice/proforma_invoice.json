 "html": "<style>\r\n    .tbspace > tbody > tr > td{ \r\n        padding: 0 2px 0 2px!important; \r\n        margin:0!important; \r\n        border-spacing: 0!important;\r\n    }\r\n    .tbspace-sub > tbody > tr > td{ \r\n        padding: 0 2px 0 2px!important; \r\n        margin:0!important; \r\n        border-spacing: 0!important;\r\n    }\r\n    .ontop {\r\n        border-top: 1px solid!important;\r\n    }\r\n    .onright {\r\n        border-right: 1px solid!important;\r\n    }\r\n    .onleft {\r\n        border-left: 1px solid!important;\r\n    }\r\n    .onbottom {\r\n        border-bottom: 1px solid!important;\r\n    }\r\n    .noright {\r\n        border-right: 0!important;\r\n    }\r\n    .noleft {\r\n        border-left: 0!important;\r\n    }\r\n    .noleftright {\r\n        border-right: 0!important; \r\n        border-left: 0!important;\r\n    }\r\n    .double {\r\n        border-bottom-style: double;\r\n        padding-bottom: 3px!important;\r\n    }\r\n    .notop {\r\n        border-top: 0!important;\r\n    }\r\n    .nobottom {\r\n        border-bottom: 0!important;\r\n    }\r\n    .notopbottom {\r\n        border-top: 0!important;\r\n        border-bottom: 0!important;\r\n    }\r\n    td.v-align {\r\n        vertical-align: middle!important;\r\n    }\r\n    .bg {\r\n        background-color: #d2d2d2!important;\r\n    }\r\n    td.nopadding{\r\n        padding: 0!important;\r\n    }\r\n    .text-size,.text{\r\n        font-size: 10px!important;\r\n    }\r\n    .bank{\r\n         line-height: 0.8!important;\r\n    }\r\n</style>\r\n<div id=\"header-html\" class=\"hidden-pdf\">\r\n    {% if letter_head and not no_letterhead %}\r\n        <div class=\"letter-head\">{{ letter_head }}</div>\r\n    {% endif %}\r\n</div>\r\n<p align=\"center\" style=\"  font-size = \"22px\">\r\n    <strong>PROFORMA INVOICE</strong>\r\n</p>\r\n<table border=\"1\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" class=\"tbspace\">\r\n    <tbody>\r\n        <tr>\r\n            <td width=\"50%\" rowspan=\"3\" valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Exporter&nbsp;:</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        {{ doc.company }}\r\n                    </strong>\r\n                </p>\r\n                 <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'address_line1') or \"\" }}\r\n                </p>\r\n                {%- if frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'address_line2') -%}\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'address_line2') or \"\" }}\r\n                </p>\r\n                {%- endif -%}\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'city') or \"\" }}-\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'pincode') or \"\" }},\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'state') or \"\" }}\r\n                     ,{{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'country')|upper or \"\" }}\r\n                </p>\r\n                <p class=\"text-size\">\r\n                        IEC NO.{{ frappe.db.get_value(\"Company\", doc.company, 'iec_code') or \"\" }}\r\n                </p>\r\n                <p class=\"text-size\">\r\n                        GSTIN No.{{ frappe.db.get_value(\"Company\", doc.company, 'gstin') or \"\" }}\r\n                </p>\r\n            </td>\r\n            <td width=\"25%\" valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Proforma Invoice</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.name|upper }} \r\n                </p>\r\n            </td>\r\n            <td width=\"25%\" valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Proforma Invoice No.&nbsp;:</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.get_formatted(\"transaction_date\")}}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td width=\"25%\"  valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Buyer's Order:</strong>\r\n                </p>\r\n                <p class=\"text-size\">{{ doc.po_no|upper if doc.po_no else '' }}</p>\r\n            </td>\r\n             <td width=\"25%\"  valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Order Date&nbsp;:</strong>\r\n                </p>\r\n                <p class=\"text-size\">{{ doc.get_formatted(\"po_date\") or '' }}</p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td width=\"25%\" valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Port Of Loading&nbsp;:</strong>\r\n                </p>\r\n                <p class=\"text\">\r\n                    {{ doc.port_of_loading or '' }}\r\n                    {% if doc.port_of_loading %}\r\n                    , {{ frappe.db.get_value(\"Port Details\",{ 'name':doc.port_of_loading },'country') or \"\" }}\r\n                    {% endif %}\r\n                </p>\r\n            </td>\r\n            <td width=\"25%\">\r\n                <p class=\"text-size\">\r\n                    <strong>Port of Discharge&nbsp;:</strong>\r\n                </p>\r\n                <p class=\"text\">\r\n                    {{ doc.port_of_discharge or '' }}\r\n                    {% if doc.port_of_loading %}\r\n                    , {{ frappe.db.get_value(\"Port Details\",{ 'name':doc.port_of_discharge },'country') or \"\" }}\r\n                    {% endif %}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td width=\"50%\" valign=\"top\"  rowspan=\"2\">\r\n                <p class=\"text\">\r\n                    <strong>Consignee&nbsp;:</strong>\r\n                </p>\r\n                <p class=\"text\">\r\n                    <strong>{{ doc.consignee_name or doc.customer_name }}</strong>\r\n                </p>\r\n                <p class=\"text\">\r\n                    {{ doc.shipping_address or '' }}\r\n                </p>\r\n            </td>\r\n            <td width=\"25%\" valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Country of Origin</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.country_of_origin or \"\"}}\r\n                </p>\r\n            \r\n            </td>\r\n             <td width=\"25%\" valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Country of Final Destination</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                   {{doc.country_of_destination or \"\"}}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td width=\"25%\" valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Partial Shipment</strong>\r\n                </p>\r\n                 <p class=\"text-size\">\r\n                    {% if doc.partial_shipment %}\r\n                    Allow\r\n                    {% else %}\r\n                    Not Allow\r\n                    {% endif %}\r\n                </p>\r\n            </td>\r\n             <td width=\"25%\" valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Trans Shipment</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {% if doc.trans_shipment %}\r\n                    Allow\r\n                    {% else %}\r\n                    Not Allow\r\n                    {% endif %}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n\r\n     <tr>\r\n            <td width=\"50%\" rowspan=\"2\" valign=\"top\">\r\n                <p class=\"text\">\r\n                    <strong>Buyer&nbsp;:</strong>\r\n                </p>\r\n                <p class=\"text\">\r\n                    <strong>{{ doc.customer_name or '' }}</strong>\r\n                </p>\r\n                <p class=\"text\">\r\n                    {{ doc.address_display or '' }} \r\n                </p>\r\n            </td>\r\n\r\n            <td width=\"25%\">\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Shiping Terms\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.shipping_terms or '' }}\r\n                   \r\n                </p>\r\n                {% if doc.shipping_instructions %}\r\n                <p  class=\"text-size\">{{ doc.shipping_instructions.replace('\\n','<br>') or '' }}</p>\r\n                {% endif %}\r\n            </td>\r\n            <td width=\"25%\">\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Packaging Instructions\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.marks_and_no or '' }}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td  width=\"50%\" colspan=\"2\">\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Payment Terms:{{ doc.payment_terms_template or \"\"}}\r\n                    </strong>\r\n                </p>\r\n               \r\n            </td>\r\n        </tr>\r\n            <td width=\"100%\" colspan=\"3\" class=\"nopadding\" style=\"padding: 0!important;\">\r\n                {% set length = doc.items|length %} {% set ln = 5 * doc.items|length %} {% set def = 200 %}\r\n                <table border=\"1\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" class=\"tbspace\" >\r\n                    <tbody>\r\n                        <tr>\r\n                            <td width=\"5%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\" >\r\n                                    <strong>Sr No.</strong>\r\n                                </p>\r\n                            </td>\r\n                            <td width=\"11%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>HSN Code</strong>\r\n                                </p>\r\n                            </td>\r\n                            <td width=\"35%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>Description of Goods</strong>\r\n                                </p>\r\n                            </td>\r\n                            \r\n\r\n                            <td width=\"10%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>Quantity</strong>\r\n                                </p>\r\n                            </td>\r\n                            <td width=\"8%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>Rate</strong>\r\n                                </p>\r\n                            </td>\r\n                            <td width=\"14%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>Total Amount</strong>\r\n                                </p>\r\n                            </td>\r\n                        </tr>\r\n                        {%- for row in doc.items -%}\r\n                        <tr style=\"height: {{ ([ln,def]|max) }}px!important;\">\r\n                            <td class=\"notopbottom\" width=\"5%\" valign=\"top\">\r\n                              <p class=\"text-size\">\r\n                                    {{ row.idx }}\r\n                                </p>\r\n                            </td>\r\n\r\n                            <td class=\"notopbottom \" width=\"11%\" valign=\"top\">\r\n                                 <p class=\"text-size\" align=\"center\">\r\n                                    {{ row.gst_hsn_code or '' }}\r\n                                </p>\r\n                            </td>\r\n                            \r\n                            <td class=\"notopbottom text-size\" width=\"40%\" valign=\"top\">\r\n                                <p class=\"text-size\">\r\n                                    <strong>{{ row.item_name or '' }}</strong><br>\r\n                                    {{row.description or \"\"}}\r\n                                    \r\n                                </p>\r\n                            </td>\r\n\r\n                            <!--<td width=\"13%\" valign=\"top\" class=\"notopbottom\">-->\r\n                            <!--    <p class=\"text-size\" align=\"center\">-->\r\n                            <!--        {{ row.packages_from}} - {{ row.packages_to }} ({{ row.no_of_packages or '' }})-->\r\n                            <!--    </p>-->\r\n                            <!--</td>-->\r\n\r\n                            <td width=\"10%\" valign=\"top\" class=\"notopbottom\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    {{ row.qty or '' }} {{ row.stock_uom or '' }}\r\n                                </p>\r\n                            </td>\r\n\r\n                            <td width=\"8%\" valign=\"top\" class=\"notopbottom\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    {{ row.get_formatted(\"rate\", doc) or ''}}\r\n                                </p>\r\n                            </td>\r\n\r\n                            <td width=\"14%\" valign=\"top\" class=\"notopbottom\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    {{ row.get_formatted(\"amount\", doc) or '' }}\r\n                                </p>\r\n                            </td>\r\n                        </tr>\r\n                        {% endfor %}\r\n                        <tr>\r\n                            <td valign=\"top\" colspan=\"2\" class=\"nobottom\">\r\n                                <p class=\"text-size\" align=\"right\">\r\n                                    <strong>Total&nbsp;&nbsp;</strong>\r\n                                </p>\r\n                            </td>\r\n\r\n                            <td valign=\"top\" class=\"nobottom\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    {{ doc.total_packages or '' }}\r\n                                </p>\r\n                            </td>\r\n\r\n                            <td valign=\"top\" class=\"nobottom\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    {{ doc.total_qty or '' }}\r\n                                </p>\r\n                            </td>\r\n                            <td valign=\"top\" class=\"nobottom\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    \r\n                                </p>\r\n                            </td>\r\n\r\n                            <td valign=\"top\" class=\"nobottom\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    {{ doc.get_formatted(\"total\", doc) or '' }}\r\n                                </p>\r\n                            </td>\r\n                        </tr>\r\n                    </tbody>\r\n                </table>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td width=\"100%\" colspan=\"3\" align=\"left\" class=\"onright\">\r\n                <table border=\"0\" width=\"100%\" cellspacing=\"0\" cellpadding=\"0\" align=\"left\" class=\"tbspace\">\r\n                    <tbody>\r\n                        <tr>\r\n                            <td width=\"20%\">\r\n                                <p class=\"text-size\">\r\n                                    <strong>Amount In Words</strong> \r\n                                </p>\r\n                            </td>\r\n                            <td width=\"80%\">\r\n                                <p class=\"text-size\">    :\r\n                                    {{ doc.in_words or '' }}\r\n                                </p>\r\n                            </td>\r\n                        </tr>\r\n                    </tbody>\r\n                </table>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n           \r\n\r\n            <td class=\"onright\" colspan=\"2\" width=\"50\" align=\"left\">\r\n\r\n                {% if doc.bank_account %}\r\n                <table border=\"0\" width=\"100%\" cellspacing=\"0\" cellpadding=\"0\" align=\"left\" class=\"tbspace\">\r\n                    <tbody>\r\n                        <tr>\r\n                            <td colspan=\"2\">\r\n                                <p style=\"font-size:8px !important; margin: 0px !important;\">\r\n                                    <strong><u> BANK DETAILS</u></strong>\r\n                                </p>\r\n                            </td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td colspan=\"2\">\r\n                                <p style=\"font-size:8px !important; margin: 0px !important;\">\r\n                                    FINAL BENIFICIARY : {{ doc.company|upper }}\r\n                                </p>\r\n                            </td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td>\r\n                                <p style=\"font-size:8px !important; margin: 0px !important;\">\r\n                                    BANK NAME : {{ frappe.db.get_value(\"My Bank Account\",{ 'name':doc.bank_account},'bank') or \"\" }}\r\n                                </p>\r\n                            </td>\r\n                            <td>\r\n                                <p style=\"font-size:8px !important; margin: 0px !important;\">\r\n                                    A/C NO : {{ doc.bank_account or \"\" }}\r\n                                </p>\r\n                            </td>\r\n                        </tr>\r\n                         <tr>\r\n                            <td colspan=\"2\">\r\n                                {% if frappe.db.get_value(\"My Bank Account\",{ 'name':doc.bank_account},'swift_code') %}\r\n                                <p style=\"font-size:8px !important; margin: 0px !important;\">\r\n                                    SWIFT CODE:{{ frappe.db.get_value(\"My Bank Account\",{ 'name':doc.bank_account},'swift_code') or \"\" }}\r\n                                </p>\r\n                                {% endif %}\r\n                            </td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td colspan=\"2\">\r\n                                <p style=\"font-size:8px !important; margin: 0px !important;\">\r\n                                    INTERMEDIARY BANK DETAILS (IF REQUIRED)\r\n                                </p>\r\n                            </td>\r\n                        </tr>\r\n                        {% if frappe.db.get_value(\"My Bank Account\",doc.bank_account,'intermediary_account') %}\r\n                        <tr>\r\n                            <td>\r\n                                <p style=\"font-size:8px !important; margin: 0px !important;\">\r\n                                    A/C NO : {{ frappe.db.get_value(\"My Bank Account\",doc.bank_account,'intermediary_account') or '' }} \r\n                                </p>\r\n                            </td>\r\n                            <td>\r\n                                <p style=\"font-size:8px !important; margin: 0px !important;\">\r\n                                    A/C WITH : {{ frappe.db.get_value(\"My Bank Account\",doc.bank_account,'intermediary_bank') or \"\" }}\r\n                                </p>\r\n                            </td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td colspan=\"2\">\r\n                                <p style=\"font-size:8px !important; margin: 0px !important;\">\r\n                                    SWIFT CODE : {{ frappe.db.get_value(\"My Bank Account\",doc.bank_account,'intermediary_swift_code') or \"\" }}\r\n                                </p>\r\n                            </td>\r\n                        </tr>\r\n                        {% endif %}\r\n                    </tbody>\r\n                </table>\r\n                \r\n                {% endif %}\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td width=\"50%\" class=\"notop noright \" align=\"left\">\r\n               <p style=\"font-size:8px !important\">\r\n                   <strong>\r\n                        This is certifiy that Quantity, Quality, Rate, Packing, Marking and all other Details of Goods are in accorance with your  Purchase Order  and Indian Origin.\r\n                   </strong>\r\n                   <br>\r\n                   We declare that the Invoice shows the actual Price of the Goods described and that all particulars are true and correct. \r\n               </p>\r\n            </td>\r\n            <td width=\"50%\" colspan=\"2\" align=\"right\">\r\n                <p style=\"font-size: 13px!important\">\r\n                    <strong>For {{ doc.company }}</strong>\r\n                  \r\n                </p>\r\n                <!--<img src=\"/private/files/COLOR SIGN.png\" style=\"max-width: 30%!important; height:auto !important; float:right\">-->\r\n                <img src=\"/files/Mukesh-Sign227e2f.png\" style=\"max-width: 40%!important; height:auto !important; float:right\">\r\n                <p class=\"text-size\" align=\"right\">\r\n                   <br><br><br>\r\n                   <br><br><br><br><br><strong>PARTNER / AUTHORISED SIGNATORY</strong>\r\n                </p>\r\n            </td>\r\n        </tr>\r\n    </tbody>\r\n</table>\r\n<div id=\"footer-html\" class=\"hidden-pdf\">\r\n    {% if not no_letterhead and footer %}\r\n        <div class=\"letter-head-footer\">\r\n            {{ footer }}\r\n        </div>\r\n    {% endif %}\r\n</div>",
