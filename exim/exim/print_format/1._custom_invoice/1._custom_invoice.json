 "doctype": "Print Format",
 "font_size": 14,
 "html": "<style>\r\n    .tbspace>tbody>tr>td {\r\n        padding: 0 2px 0 2px !important;\r\n        margin: 0 !important;\r\n        border-spacing: 0 !important;\r\n    }\r\n\r\n    .tbspace-sub>tbody>tr>td {\r\n        padding: 0 4px 0 4px !important;\r\n        margin: 0 !important;\r\n        border-spacing: 0 !important;\r\n    }\r\n    /*.print-format {*/\r\n        /*margin-top: 20mm !important;*/\r\n    /*    margin-bottom: -1mm !important;*/\r\n        /*margin-left: -1mm !important;*/\r\n    /*    margin-right: 0mm !important;*/\r\n    /*}*/\r\n\r\n    .ontop {\r\n        border-top: 1px solid !important;\r\n    }\r\n\r\n    .onright {\r\n        border-right: 1px solid !important;\r\n    }\r\n\r\n    .onleft {\r\n        border-left: 1px solid !important;\r\n    }\r\n\r\n    .onbottom {\r\n        border-bottom: 1px solid !important;\r\n    }\r\n\r\n    .noright {\r\n        border-right: 0 !important;\r\n    }\r\n\r\n    .noleft {\r\n        border-left: 0 !important;\r\n    }\r\n\r\n    .noleftright {\r\n        border-right: 0 !important;\r\n        border-left: 0 !important;\r\n    }\r\n\r\n    .notop {\r\n        border-top: 0 !important;\r\n    }\r\n\r\n    .nobottom {\r\n        border-bottom: 0 !important;\r\n    }\r\n\r\n    .notopbottom {\r\n        border-top: 0 !important;\r\n        border-bottom: 0 !important;\r\n    }\r\n\r\n    td.v-align {\r\n        vertical-align: middle !important;\r\n    }\r\n\r\n    .bg {\r\n        background-color: #d2d2d2 !important;\r\n    }\r\n\r\n    td.nopadding {\r\n        padding: 0 !important;\r\n    }\r\n\r\n    td.v-align-bt {\r\n        vertical-align: bottom !important;\r\n    }\r\n\r\n    .text-size {\r\n        font-size: 9px !important\r\n    }\r\n    .no-margin{\r\n        margin: 0px !important;\r\n    }\r\n</style>\r\n{% set lst = ['ORIGINAL FOR RECIPIENT','DUPLICATE FOR TRANSPORTER','TRIPLICATE FOR SUPPLIER'] %}\r\n{% for row in lst %}\r\n<div id=\"header-html\" class=\"hidden-pdf\" style=\"padding-top:20px !important;\">\r\n   {% if letter_head %}\r\n   <div class=\"letter-head\">{{ letter_head }}</div>\r\n   {%else%}\r\n   <div style=' margin-top: 20mm !important;'></div>\r\n   {% endif %}\r\n</div>\r\n\r\n\r\n<table border=\"1\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" class=\"tbspace\">\r\n    <tbody>\r\n  \r\n        <tr>\r\n            <!--<td width=\"32%\" class=\"v-align ontop onleft \">-->\r\n            <!--    <p class=\"text-size\">-->\r\n                    <!--<strong>-->\r\n                    <!--    {{ doc.company }}-->\r\n                    <!--</strong>-->\r\n            <!--    </p>-->\r\n            <!--</td>-->\r\n            <td width=\"62%\" valign=\"top\" colspan =\"2\">\r\n                <p class=\"text-size\" align=\"center\">\r\n                    {% if doc.gst_category == 'Overseas'  or  doc.gst_category == 'Deemed Export' %}\r\n                    <strong>EXPORT INVOICE</strong>\r\n                    {% else %}\r\n                    INVOICE\r\n                    {% endif %}\r\n                \r\n                </p>\r\n            </td>\r\n            <td width=\"30%\" valign=\"top\">\r\n                <p class=\"text-size\" align=\"left\">\r\n                    <span class=\"sqare\">&#x2611;</span>{{ row }}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td width=\"40%\" rowspan=\"3\" valign=\"top\">\r\n                <p class=\"text-size\"><strong>Exporter :</strong></p>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        {{ doc.company }}\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'address_line1') or \"\" }}\r\n                </p>\r\n                {%- if frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'address_line2') -%}\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'address_line2') or \"\" }}\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'city') or \"\" }}-\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'pincode') or \"\" }},\r\n                </p>\r\n                {%- endif -%}\r\n             \r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'state') or \"\" }}\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    <strong>GST No. : {{ doc.company_gstin or '' }}</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    UDYAM REGISTRATION NUMBER:\r\n                    {{ frappe.db.get_value(\"Company\",doc.company,'msme_registration_number') or ''}}\r\n                </p>\r\n                    {% if doc.company_tax_id %}\r\n                                <p class=\"text-size\">\r\n                                    PAN NO : {{ doc.company_tax_id  }}\r\n                                </p>\r\n                                {% endif %}\r\n\r\n            </td>\r\n            <td width=\"30%\" valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Invoice No. &amp;  Date :</strong>\r\n                </p>\r\n                <p class=\"text-size no-margin\">\r\n                    {{ doc.name }}&nbsp;&nbsp;&amp;&nbsp;{{ doc.get_formatted(\"posting_date\") }}\r\n                </p>\r\n            </td>\r\n            <td width=\"30%\" valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Container No.</strong>\r\n                </p>\r\n                <p class=\"text-size no-margin\">\r\n                    {% for row in doc.container_detail %}\r\n                        {{ row.container_no or ''}}\r\n                    {% endfor %}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Buyer's Order No. & Date :</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.po_no|upper if doc.po_no else '' }}&nbsp;&nbsp;&nbsp;{{ doc.get_formatted(\"po_date\") or '' }}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\"><strong>Exporter Ref no and Date:</strong></p>\r\n                 {% if doc.items[0].sales_order %}\r\n                <p class=\"text-size no-margin\">\r\n                    \r\n                    {{ frappe.db.get_value(\"Sales Order\",doc.items[0].sales_order,'order_no') or doc.items[0].sales_order or '' }}&nbsp;&nbsp;&nbsp;{{ frappe.format(frappe.db.get_value(\"Sales Order\",doc.items[0].sales_order,'transaction_date'), {'fieldtype': 'Date'}) }}\r\n                </p>\r\n                {% endif %}\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Country of origin:</strong>\r\n                </p>\r\n                <p class=\"text-size no-margin\">{{ doc.country_of_origin|upper if doc.country_of_origin else '' }}</p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\"><strong>Country of Final Destination :</strong></p>\r\n                <p class=\"text-size no-margin\">{{ doc.country_of_destination|upper if doc.country_of_destination else '' }}</p>\r\n            </td>\r\n        </tr>\r\n        \r\n        <tr>\r\n            <td rowspan=\"2\">\r\n                <p class=\"text-size\">\r\n                    <strong>Consignee :</strong>\r\n                </p>\r\n                <p class=\"text-size\"><strong>TO THE ORDER</strong></p>\r\n                <p class=\"text-size\"><strong>{{ doc.country_of_destination }}</strong></p>\r\n\r\n                <!--<p class=\"text-size no-margin\">-->\r\n                <!--    {{ doc.port_of_discharge|upper if doc.port_of_discharge else '' }}-->\r\n                <!--</p>-->\r\n                 \r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>Terms of Delivery and Payment</strong>\r\n                </p>\r\n                <p class=\"text-size no-margin\">\r\n                    PAYMENT : {{ doc.payment_terms_template|upper if doc.payment_terms_template else '' }}\r\n                </p>\r\n                <p class=\"text-size no-margin\">\r\n                    DELIVERY :    {% if doc.shipping_terms == \"FOB\" %}\r\n                    {{ doc.shipping_terms or ''}} {{ doc.port_of_loading or '' }}\r\n                    {% elif doc.shipping_terms == \"Ex-Works\"%} \r\n                    {{ doc.shipping_terms or ''}}\r\n                    {% else %}\r\n                    {{ doc.shipping_terms or ''}} {{ doc.port_of_discharge or '' }}\r\n                    {% endif %}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong> Transporter Name :</strong>\r\n            \r\n                </p>\r\n                <p class=\"text-size \">\r\n                    {{ doc.transporter_name or '' }}\r\n                </p>\r\n            </td>\r\n\r\n        </tr>\r\n        <tr>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Pre Carriage by\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size no-margin\">\r\n                    {{ doc.pre_carriage_by or '' }}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>Vehicle No. :</strong> {{ doc.vehicle_no or '' }}\r\n                </p>\r\n               \r\n                <p class=\"text-size\">\r\n                    <strong>LR No. :</strong>\r\n                    {{ doc.lr_no or ''}}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td width=\"40%\" rowspan=\"2\">\r\n                <p class=\"text-size\">\r\n                    <strong>Buyer</strong>(if other than Consignee)\r\n                </p>\r\n                 <p class=\"text-size\"><strong>TO THE ORDER</strong></p>\r\n                <p class=\"text-size no-margin\">\r\n                {{ doc.final_destination|upper if doc.final_destination else '' }}\r\n                    <!--{{ doc.shipping_address|upper if doc.shipping_address else ''}}-->\r\n                </p>\r\n                                <p class=\"text-size\"><strong>{{ doc.country_of_destination }}</strong></p>\r\n\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>Vessel/Flight No.</strong></p>\r\n                <p class=\"text-size no-margin\">\r\n                    {{ doc.vessel_no|upper if doc.vessel_no else '' }}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\"><strong>Port of Loading</strong></p>\r\n                <p class=\"text-size no-margin\">\r\n                    {{ doc.port_of_loading|upper if doc.port_of_loading else '' }}\r\n                    {% if frappe.db.get_value(\"Port Details\",doc.port_of_loading,\"country\") %}\r\n                    ,{{ frappe.db.get_value(\"Port Details\",doc.port_of_loading,\"country\")|upper if frappe.db.get_value(\"Port Details\",doc.port_of_loading,\"country\") else '' }}\r\n                    {% endif %}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        \r\n        <tr>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Port of Discharge\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size no-margin\">\r\n                    {{ doc.port_of_discharge|upper if doc.port_of_discharge else '' }}\r\n                    {% if frappe.db.get_value(\"Port Details\",doc.port_of_discharge,\"country\") %}\r\n                    ,{{ frappe.db.get_value(\"Port Details\",doc.port_of_discharge,\"country\")|upper if frappe.db.get_value(\"Port Details\",doc.port_of_discharge,\"country\") else '' }}\r\n                    {% endif %}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Final Destination\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size no-margin\">\r\n                    {{ doc.final_destination|upper if doc.final_destination else '' }}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td width=\"100%\" colspan=\"3\" style=\"padding: 0!important;\">\r\n            {% set length = doc.items|length %} {% set ln = 5 * doc.items|length %} {% set def = 150 %}\r\n                <table border=\"1\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" class=\"tbspace\">\r\n                    <tbody>\r\n                        <tr>\r\n                            <td width=\"5%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>SR No</strong>\r\n                                </p>\r\n                            </td>\r\n                            <td width=\"15%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>MARKS & No/<br>Container No.</strong>\r\n                                </p>\r\n                            </td>\r\n                            <td width=\"13%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>No. & Kind of<br>Packages</strong>\r\n                                </p>\r\n                            </td>\r\n                            <td width=\"26%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>Description of Goods</strong>\r\n                                </p>\r\n                                \r\n                            </td>\r\n\r\n                            <td width=\"8%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>Qty {{ doc.items[0].uom }}\r\n                                    </strong>\r\n                                </p>\r\n                            </td>\r\n                            <td width=\"8%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>Rate<br>PER {{ doc.items[0].uom }}</strong>\r\n                                </p>\r\n                            </td>\r\n                        \r\n                            <td width=\"10%\" class=\"v-align\" align=\"center\">\r\n                                <p class=\"text-size\">\r\n                                    <strong>Amount</strong>\r\n                                    <!--<strong>{{ doc.currency }}</strong>-->\r\n                                </p>\r\n                            </td>\r\n                            <!--<td width=\"10%\" class=\"v-align\" align=\"center\">-->\r\n                            <!--    <p class=\"text-size\">-->\r\n                            <!--        <strong>IGST Amount</strong>-->\r\n                            <!--    </p>-->\r\n                            <!--</td>-->\r\n                        </tr>\r\n                    \r\n                       \r\n                        {% set count = namespace(a=0)%}\r\n                        {% set pallets_sum = namespace(value=0) %}\r\n                        {% for row in doc.items %}\r\n                       \r\n                        {% set count.a = count.a + 1 %}\r\n                       \r\n                        <tr style=\"height: {{ ([ln,def]|max) }}px!important;\">\r\n                            <td class=\"notopbottom\">\r\n                                <p class = \"notopbottom text-size\" align=\"center\"> {{ row.idx }}</p>\r\n                            </td>\r\n                            <td  class=\"notopbottom onleftright\">\r\n                                <p class=\"text-size\">\r\n                                    \r\n                                    {% if row.marks_and_no %}\r\n                                    \r\n                                    {% if loop.last %}{{ row.marks_and_no.replace(\"\\n\",\"<br>\") }} {% else %} {{ row.marks_and_no.replace(\"\\n\",\"<br>\") }} {% endif %}\r\n                                    {% endif %}\r\n                                </p>\r\n                            </td>\r\n                            <td  class=\"notopbottom onright\">\r\n                                <p align=\"center\" class=\"text-size\">\r\n                                    \r\n                                   \r\n                                         {{row.no_of_packages or \"\"}}  {{ row.packaging_material or ''}} {% if row.packing_size %} of {% endif %}\r\n                                            {{ row.packing_size or ''}} {{row.uom or \"\"}}\r\n                                           \r\n                                            {% if row.palleted %}\r\n                                               \r\n                                                <br> each on {{row.total_pallets or \"\"}} pallets\r\n                                            {% endif %}\r\n                                </p>\r\n                            </td>\r\n                            <td  class=\"notopbottom onright text-size\">   \r\n                                    <p class=\"text-size no-margin\">\r\n                                        {{row.description or row.item_code or \"\"}}\r\n                                    </p>\r\n                                \r\n                                <p class=\"text-size no-margin\">\r\n                                    (HS Code: {{ row.gst_hsn_code or '' }})\r\n                                </p>\r\n                                \r\n                            </td>\r\n                            <td  class=\"notopbottom onright\">\r\n                                <p align=\"center\" class=\"text-size\">\r\n                                    <!--{%- set sum_qty = [0] -%}-->\r\n                                    <!--{% for d in doc.items %}-->\r\n                                    <!--{% if d.item_code == row.item_code and d.rate == row.rate and d.lot_no == row.lot_no  %}-->\r\n                                    <!--{%- set _ = sum_qty.append(sum_qty.pop() + d.qty) -%}-->\r\n                                    <!--{% endif %}-->\r\n                                    <!--{% endfor %}-->\r\n                                    <!--{{ sum_qty[0] }}-->\r\n                                     {{ row.qty or '' }}\r\n                                </p>\r\n                            </td>\r\n                            <td class=\"notopbottom onright\">\r\n                                <p align=\"right\" class=\"text-size\">\r\n                                    {{ row.get_formatted(\"rate\") or '' }}\r\n                                </p>\r\n                            </td>\r\n                            <td  class=\"notopbottom onleftright\">\r\n                                <p align=\"right\" class=\"text-size\">\r\n                                    <!--{%- set sum_amount = [0] -%}-->\r\n                                    <!--{% for d in doc.items %}-->\r\n                                    <!--{% if d.item_code == row.item_code and d.rate == row.rate and d.lot_no == row.lot_no  %}-->\r\n                                    <!--{%- set _ = sum_amount.append(sum_amount.pop() + d.amount) -%}-->\r\n                                    <!--{% endif %}-->\r\n                                    <!--{% endfor %}-->\r\n                                    <!--{{ frappe.utils.fmt_money(sum_amount[0],currency=doc.currency) }}-->\r\n                                    <!--{{ frappe.format(sum_amount[0], {'fieldtype': 'Currency'}) }}-->\r\n                                    {{ row.get_formatted(\"amount\") or '' }}\r\n                                </p>\r\n                            </td>\r\n                          \r\n                            \r\n                            \r\n                        </tr>\r\n                        {% endfor %}\r\n                        <tr>\r\n                            \r\n                            <td valign=\"top\" align=\"center\" class=\"notopbottom\">\r\n                            </td>\r\n                            <td valign=\"top\" align=\"center\" class=\"notopbottom\">\r\n\r\n                            </td>\r\n                            <td valign=\"top\" class=\"notopbottom\">\r\n                                <p class=\"text-size\">\r\n\r\n                                </p>\r\n                                <p class=\"text-size\">\r\n                                    <strong>{% if doc.dbk_sr_no %} Export Under Duty DBK Scheme {% endif %}</strong>\r\n                                </p>\r\n                            </td>\r\n\r\n                            <td valign=\"top\" align=\"center\" class=\"notopbottom\">\r\n\r\n                            </td>\r\n                        \r\n                            <td class=\"notopbottom\" align=\"right\">\r\n                               <p class=\"text-size\">\r\n                                Freight \r\n                                </p>\r\n                                <p class=\"text-size\">\r\n                                Insurance \r\n                                </p>\r\n                                <p class=\"text-size\">\r\n                                    Commission\r\n                               </p>\r\n                            </td>\r\n                            <td class=\"notopbottom\" align=\"right\">\r\n                                <p class=\"text-size\">\r\n                                    {{doc.get_formatted(\"freight\") or \"\"}}\r\n                                </p>\r\n                                <p class=\"text-size\">\r\n                                   {{doc.get_formatted(\"insurance\") or \"\"}}\r\n                                </p>\r\n                                <p class=\"text-size\">\r\n                                    {{doc.get_formatted(\"commission\") or \"\"}}\r\n                                </p>\r\n                                \r\n                            </td>\r\n                            <td class=\"notopbottom\" align=\"right\">\r\n\r\n                            </td>\r\n                        </tr>\r\n\r\n                        <tr>\r\n                            <td width=\"5%\"></td>\r\n                            <td valign=\"top\">\r\n                                <p class=\"text-size\" align=\"right\">\r\n                                    <strong>Total&nbsp;&nbsp;</strong>\r\n                                </p>\r\n                            </td>\r\n                            <td valign=\"top\" align=\"center\">\r\n                                <p class=\"text-size\"><strong> {{  doc.total_pallets or \"\"}}\r\n                                        Pallets</strong>\r\n                           